{
  "name": "v3-admin-vite-js",
  "version": "4.5.6",
  "description": "一个免费开源的中后台管理系统基础解决方案，基于 Vue3、Element Plus、Pinia 和 Vite 等主流技术",
  "author": {
    "name": "pany",
    "email": "939630029@qq.com",
    "url": "https://github.com/pany-ang"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pany-vip/v3-admin-vite-4.5.6-js.git"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:stage": "vite build --mode staging",
    "build:prod": "vite build",
    "build:gas": "powershell -Command \"$env:VITE_BUILD_FOR_GAS='true'; vite build --mode gas\"",
    "deploy:gas": "npm run build:gas && powershell -File gas.ps1 && clasp push",
    "preview:stage": "pnpm build:stage && vite preview",
    "preview:prod": "pnpm build:prod && vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"src/**/*.{vue,js,jsx}\" --fix",
    "lint:prettier": "prettier --write \"src/**/*.{vue,js,jsx,json,css,less,scss,html,md}\"",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "prepare": "husky"
  },
  "dependencies": {
    "@element-plus/icons-vue": "2.3.1",
    "dayjs": "1.11.13",
    "element-plus": "2.8.8",
    "js-cookie": "3.0.5",
    "lodash-es": "4.17.21",
    "mitt": "3.0.1",
    "normalize.css": "8.0.1",
    "nprogress": "0.2.0",
    "path-browserify": "1.0.1",
    "path-to-regexp": "8.2.0",
    "pinia": "2.2.6",
    "pnpm": "10.27.0",
    "screenfull": "6.0.2",
    "vue": "3.5.13",
    "vue-router": "4.4.5"
  },
  "devDependencies": {
    "@google/clasp": "3.1.3",
    "@types/google-apps-script": "2.0.5",
    "@typescript-eslint/eslint-plugin": "8.12.2",
    "@typescript-eslint/parser": "8.12.2",
    "@vitejs/plugin-vue": "5.2.0",
    "@vitejs/plugin-vue-jsx": "4.1.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "13.0.0",
    "eslint": "8.57.1",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-vue": "9.31.0",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "sass": "1.78.0",
    "unocss": "0.64.1",
    "vite": "5.4.11",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-svg-loader": "5.1.0",
    "vue-eslint-parser": "9.4.3"
  },
  "lint-staged": {
    "src/**/*.{vue,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "*.{css,less,scss,html,md}": [
      "prettier --write"
    ],
    "package.json": [
      "prettier --write"
    ]
  },
  "keywords": [
    "vue",
    "vue3",
    "admin",
    "vue-admin",
    "vue3-admin",
    "vite",
    "vite-admin",
    "element-plus",
    "element-plus-admin"
  ]
}
